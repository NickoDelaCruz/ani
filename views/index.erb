<form  action='/result'>
  <h4>Plan your trip: WHere are you going?</h4>
  <div class="form-group">
    <input id="name" name="name" type="text" placeholder="Enter your name">
  </div>
  <div class="form-group">
    <input id="city_name" name="city_name" type="text" placeholder="Enter city name">
  </div>
  <div class="form-group">
    <input id="state_name" name="state_name" type="text" placeholder="Enter state">
  </div>
  <div class="form-group">
    <input id="latitude" name="latitude" type="text" placeholder="Enter location latitude">
  </div>
  <div class="form-group">
    <input id="longitude" name="longitude" type="text" placeholder="Enter location longitude">
  </div>
  <button type="submit" name="submit" class="btn btn-success">Submit</button>
</form>
